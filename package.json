{
  "name": "easy-descriptor",
  "version": "1.0.2-beta.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc -p tsconfig.build.json",
    "build:push": "vite build && tsc -p tsconfig.build.json && yalc publish --push",
    "version": "npm version patch",
    "push": "npm publish --registry https://registry.npmjs.org/",
    "push:beta": "npm publish --tag beta --registry https://registry.npmjs.org/"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.js"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "default": "./dist/index.es.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlpsDDJ/easy-descriptor.git"
  },
  "keywords": [
    "easy-descriptor",
    "decorator"
  ],
  "bugs": {
    "url": "https://github.com/AlpsDDJ/easy-descriptor/issues"
  },
  "homepage": "https://github.com/AlpsDDJ/easy-descriptor",
  "author": "AlpsDDJ",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "lodash-es": "^4.17.21",
    "typescript": "^5.4.5",
    "vite": "^5.2.10",
    "vite-plugin-dts": "^3.9.1"
  }
}
